{
  "name": "goodkind.io",
  "private": true,
  "version": "3.0.0",
  "scripts": {
    "static:build": "ts-node build.ts",
    "static:serve": "ts-node dev-serve.ts",
    "static:dev": "nodemon",
    "lint": "eslint src build.ts *.ts --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "clean": "find . -type d -name node_modules -prune -o -name dist -exec rm -rf {} \\+"
  },
  "exports": {
    "./*": "./dist/*"
  },
  "engines": {
    "node": ">=21.7.1"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "autoprefixer": "^10.4.18",
    "clsx": "^2.1.0",
    "cssnano": "^6.1.0",
    "express": "^4.18.3",
    "favicons": "^7.1.5",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "prettier-eslint": "^16.3.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
