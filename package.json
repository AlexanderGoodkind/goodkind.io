{
  "name": "goodkind.io",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "find . -type d -name node_modules -prune -o -name dist -exec rm -rf {} \\+"
  },
  "exports": {
    "./*": "./dist/*"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.1.0",
    "favicons": "^7.1.5",
    "node": "^21.6.2",
    "npm": "^10.3.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.1"
  }
}
