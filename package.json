{
  "name": "goodkind.io",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "NO_COLOR=true vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "find . -type d -name node_modules -prune -o -name dist -exec rm -rf {} \\+"
  },
  "exports": {
    "./*": "./dist/*"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "clsx": "^2.1.0",
    "eslint": "^8.56.0",
    "favicons": "^7.1.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.3"
  }
}